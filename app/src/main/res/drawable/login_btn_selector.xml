<?xml version="1.0" encoding="utf-8"?>
<!-- 给button添加点击效果-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/login_btn_style_press" android:state_selected="true"/>
    <item android:drawable="@drawable/login_btn_style_press" android:state_focused="true"/>
    <item android:drawable="@drawable/login_btn_style_press" android:state_pressed="true"/>
    <item android:drawable="@drawable/login_btn_style_press" android:state_checked="true"/>
    <item android:drawable="@drawable/login_btn_style_normal"/>

</selector>